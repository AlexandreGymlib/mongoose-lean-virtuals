    "lean",
    "virtuals",
    "mongodb"
  ],
  "devDependencies": {
    "acquit": "1.x",
    "acquit-ignore": "0.1.0",
    "acquit-markdown": "0.1.0",
    "co": "4.6.0",
    "istanbul": "0.4.5",
    "mocha": "5.2.x",
    "mongoose": "5.4.x"
  },
  "peerDependencies": {
    "mongoose": ">=4.11.0 || 5.x"
  },
  "author": "Valeri Karpov <val@karpov.io>",
  "license": "Apache 2.0",
  "bugs": {
    "url": "https://github.com/vkarpov15/mongoose-lean-virtuals/issues"
